{
  "name": "@mongoosejs/netlify-functions-ecommerce",
  "version": "1.0.0",
  "license": "MIT",
  "dependencies": {
    "@datastax/astra-mongoose": "1.1.0",
    "dotenv": "17.2.3",
    "mongoose": "^8.14",
    "sinon": "21.0.0",
    "stripe": "19.1.0",
    "vanillatoasts": "1.6.0",
    "webpack": "5.x"
  },
  "devDependencies": {
    "@masteringjs/eslint-config": "0.1.1",
    "eslint": "9.38.0",
    "mocha": "11.7.4",
    "netlify-cli": "23.10.0"
  },
  "engines": {
    "node": ">=20.6.0"
  },
  "scripts": {
    "build": "node --env-file .env ./frontend/build",
    "clean-db": "node --env-file .env ./dropCollections",
    "lint": "eslint .",
    "seed": "node --env-file .env ./seed",
    "start": "netlify dev --dir public --functions netlify/functions",
    "test": "env NODE_ENV=test node --env-file=.env.test ./node_modules/.bin/mocha ./test/*.test.js",
    "test:smoke": "node --env-file .env ./smoke-test"
  }
}
